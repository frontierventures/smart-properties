<div id="sellerInfoPopup" class="colorbox-zone" xmlns:t="http://twistedmatrix.com/ns/twisted.web.template/0.1">
    <form id="sellerInfoForm" action="../saveBuyerInfo" method="POST" enctype="application/x-www-form-urlencoded">
        <div class="c12 inner-content">
            <ul class="settings-list addnew-prod">
                <li>
                    <div class="c12">
                        <div class="c8">
                            <h1 class="c12">Seller Information</h1>
                        </div>
                    </div>
                    <div class="c12">
                        Please provide the following information before you can start selling
                    </div>
                </li>
                <li>
                    <div id="sellerNotification">
                    </div>
                    <div class="c12">
                        <div class="c3">
                            <label for="first">First Name:</label>
                        </div>
                        <div class="c6">
                            <input type="text" name="sellerFirst" id="first"/>
                        </div>
                    </div>
                    <div class="c12">
                        <div class="c3">
                            <label for="last">Last Name:</label>
                        </div>
                        <div class="c6">
                            <input type="text" name="sellerLast" id="last"/>
                        </div>
                    </div>
                </li>
                <li class="full center">
                    <button class="button green medium" name="button" value="Save">Update Information</button>
                </li>
            </ul>
        </div>
    </form>
</div>
