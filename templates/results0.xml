<div style="float: left" xmlns:t="http://twistedmatrix.com/ns/twisted.web.template/0.1">
    <div style="margin-left: 50px;" class="c2x">
        <div style="height: 420px;"></div>
        <div>
            <a href="../seller"><img src="../images/coingig_banner.png"></img></a>
        </div>
        <div style="text-align: center;" t:render="topStore">
            <br />
            <div style="font-size: 24px; font-weight: bold;">
                Top Store
            </div>
            <div>
                <a><t:attr name="href"><t:slot name="htmlStoreUrl" /></t:attr>
                    <img><t:attr name="src"><t:slot name="htmlStoreLogo" /></t:attr></img>
                </a>
            </div>
            <div>
                <a><t:attr name="href"><t:slot name="htmlStoreUrl" /></t:attr>
                    <b><t:slot name="htmlStoreName" /></b>
                </a>
            </div>
            <br />
        </div>
    </div>
    <div class="content" style="overflow: hidden;">
        <t:transparent t:render="sortMenu" />
        <ul class="products" style="float: left;">
        </ul>
        <ul class="pagination">
        </ul>
    </div>
</div>
