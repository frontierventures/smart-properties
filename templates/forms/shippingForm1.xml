<table class="c12 acctable invoice-table" xmlns:t="http://twistedmatrix.com/ns/twisted.web.template/0.1">
    <thead>
        <tr>
            <th colspan="2">Shipping Information</th>
        </tr>
    </thead>

    <tbody>
        <tr t:render="shippingDetails">
            <td>
                <span>Shipping Carrier</span>
                <div>
                    <t:slot name="htmlShippingCarrier" />
                </div>
            </td>
            <td>
                <span>Tracking Number</span>
                <div>
                    <t:slot name="htmlTrackingNumber" />
                </div>
            </td>
        </tr>
    </tbody>
</table>
