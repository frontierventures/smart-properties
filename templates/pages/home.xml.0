    <!-- Carousel
    ================================================== -->
    <div id="myCarousel" class="carousel slide">
        <div class="carousel-inner">
            <div class="item active">
                <img src="../resources/img/slides/1.jpg" alt="" />
                <div class="container">
                    <div class="carousel-caption">
                        <h1>Diversify</h1>
                        <p class="lead">Invest in us and expand your bitcoin portfolio</p>
                        <a class="btn btn-large btn-primary" href="../register">Sign up today</a>
                    </div>
                </div>
            </div>
            <div class="item">
                <img src="../resources/img/slides/2.jpg" alt="" />
                <div class="container">
                    <div class="carousel-caption">
                        <h1>Profit</h1>
                        <p class="lead">Expect safe and secure fiat APR of 10.2%</p>
                        <a class="btn btn-large btn-primary" href="../legal">Learn more</a>
                    </div>
                </div>
            </div>
            <div class="item">
                <img src="../resources/img/slides/3.jpg" alt="" />
                <div class="container">
                    <div class="carousel-caption">
