<div class="c12" xmlns:t="http://twistedmatrix.com/ns/twisted.web.template/0.1">
    <div class="title-center">
        <h3>Customer Reviews</h3>
    </div>
    <ul class="reviews c12">
        <li t:render="review">
            <div class="c12">
                <div class="rating-zone">
                    <div class="rating">
                        <div class="stars">
                            <t:attr name="style"><t:slot name="htmlStarsStyle" /></t:attr>
                        </div>
                    </div>
                </div>
                <h4><t:slot name="htmlReviewHeadline" /></h4>
            </div>
            <div class="c12">
                By <t:slot name="htmlBuyerFirst" />
                <p><t:slot name="htmlReviewBody" /></p>
            </div>
        </li>
    </ul>
</div>
